:scss

    .frame2{
    display: black;
    width: 450px;
    height: 600px;
    background: #E6F2FF;
    margin: 20px;
    position: absolute;
    }
    
    #topimg{
    position: relative;
    left: 35px;
    width: 360px;
    height: 50px;
    }
    
    #instructions-panel{
    position: relative;
    background: white;
    height: 90%;
    width: auto;
    margin: 10px 10px 10px 10px;
    }
    
    #header{
    position: relative;
    width: 300px;
    height: 100px;
    left: 63px;
    top: 30px;
    background: linear-gradient(180deg, #FFFFFF 0%, rgba(255, 255, 255, 0) 100%), #AFFFD8;
    border: 2px solid #000000;
    box-shadow: 0px 4px 20px rgba(0, 0, 0, 0.25);
    }
    
    #pic-content{
    position: relative;
    width: 300px;
    height: 300px;
    left: 63px;
    margin-top: 70px;
    background: linear-gradient(180deg, #FFFFFF 0%, rgba(255, 255, 255, 0) 100%), #AFFFD8;
    border: 2px solid #000000;
    box-shadow: 0px 4px 20px rgba(0, 0, 0, 0.25);
    }
    
    .description
    {
    position: relative;
    font-size: 22px;
    font-weight: bold;
    margin: 20px;
    margin-top: 12px;
    text-align: center;
    }
    
    .description2
    {
    position: relative;
    }

    .sub-step{
    padding: 0px;
    margin: 0px;
    }

    #img{
    position: relative;
    width: 80px;
    height: 60px;
    left: 290px;
    }
    #img2{
    position: relative;
    width: 97px;
    height: 97px;
    }

.frame2
    %img#topimg{src:"/Top Navigation Bar.svg"}
    .instructions-panel
        #header
            .description
                %p.sub-step Beginner's Guide 
                %p.sub-step to 
                %p.sub-step Pot Stickers
        #pic-content
            .description2
                %img#img2{src:"/Top left pic.png"}
                %img#img2{src:"/Top center pic.png"}
                %img#img2{src:"/Top right pic.png"}
            .description2
                %img#img2{src:"/Center left pic.png"}
                %img#img2{src:"/Center pic.png"}
                %img#img2{src:"/Center right pic.png"}            
            .description2
                %img#img2{src:"/Bottom left pic.png"}
                %img#img2{src:"/Bottom center pic.png"}
                %img#img2{src:"/Bottom right pic.png"}
        %img#img{src:"/forward-arrow.svg"}
            
